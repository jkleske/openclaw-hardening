{
  "//": "Read-only agent: can respond in channels but cannot modify files or run commands.",
  "//silent": "To make the agent completely silent (CLI-only), add 'message' to the deny list.",
  "//note": "Merge this into your agents.list[] entry in openclaw.json.",

  "id": "YOUR_AGENT_ID",

  "tools": {
    "profile": "minimal",

    "allow": [
      "read",
      "memory_search"
    ],

    "deny": [
      "write",
      "edit",
      "apply_patch",
      "exec",
      "bash",
      "process",
      "cron",
      "gateway",
      "sessions_spawn",
      "sessions_send",
      "browser",
      "canvas",
      "nodes",
      "web_search",
      "web_fetch",
      "image"
    ],

    "fs": {
      "workspaceOnly": true
    },

    "elevated": {
      "enabled": false
    }
  },

  "memorySearch": {
    "extraPaths": []
  }
}
